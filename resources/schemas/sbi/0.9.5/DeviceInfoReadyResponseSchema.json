{
	"$schema": "https://json-schema.org/draft/2019-09/schema#",
	"$id": "http://mosip-compliance-toolkit.com/schemas/DeviceInfoReadyResponseSchema.json",
	"title": "DeviceInfo response Schema",
	"description": "DeviceInfo response Schema",
	"additionalProperties": false,
	"type": "array",
	"minItems": 1,
	"items": [
		{
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"deviceInfo": {
					"$ref": "#/definitions/non-empty-string"
				},
				"deviceInfoDecoded": {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"env": {
							"type": "string",
							"enum": [
								"Staging",
								"Developer",
								"Pre-Production",
								"Production"
							]
						},
						"digitalId": {
							"$ref": "#/definitions/non-empty-string"
						},
						"digitalIdDecoded": {
							"type": "object",
							"additionalProperties": false,
							"properties": {
								"serialNo": {
									"$ref": "#/definitions/non-empty-string"
								},
								"make": {
									"$ref": "#/definitions/non-empty-string"
								},
								"model": {
									"$ref": "#/definitions/non-empty-string"
								},
								"type": {
									"type": "string",
									"enum": [
										"Finger",
										"Iris",
										"Face"
									]
								},
								"deviceSubType": {
									"type": "string",
									"enum": [
										"Slap",
										"Single",
										"Touchless",
										"Double",
										"Full face"
									]
								},
								"deviceProvider": {
									"$ref": "#/definitions/non-empty-string"
								},
								"deviceProviderId": {
									"$ref": "#/definitions/non-empty-string"
								},
								"dateTime": {
									"type": "string",
									"format": "date-time",
									"description": "We expect YYYY-MM-ddTHH:mm:SSZ"
								}
							},
							"allOf": [
								{
									"if": {
										"properties": {
											"type": {
												"const": "Finger"
											}
										}
									},
									"then": {
										"properties": {
											"deviceSubType": {
												"type": "string",
												"enum": [
													"Single",
													"Slap",
													"Touchless"
												]
											}
										}
									}
								},
								{
									"if": {
										"properties": {
											"type": {
												"const": "Iris"
											}
										}
									},
									"then": {
										"properties": {
											"deviceSubType": {
												"type": "string",
												"enum": [
													"Single",
													"Double"
												]
											}
										}
									}
								},
								{
									"if": {
										"properties": {
											"type": {
												"const": "Face"
											}
										}
									},
									"then": {
										"properties": {
											"deviceSubType": {
												"type": "string",
												"enum": [
													"Full face"
												]
											}
										}
									}
								}
							],
							"required": [
								"serialNo",
								"make",
								"type",
								"model",
								"deviceSubType",
								"deviceProviderId",
								"deviceProvider",
								"dateTime"
							],
							"definitions": {
								"non-empty-string": {
									"type": "string",
									"minLength": 1
								}
							}
						},
						"purpose": {
							"type": "string",
							"enum": [
								"Auth",
								"Registration"
							]
						},
						"serviceVersion": {
							"$ref": "#/definitions/non-empty-string"
						},
						"deviceStatus": {
							"type": "string",
							"enum": [
								"Ready"
							]
						},
						"firmware": {
							"$ref": "#/definitions/non-empty-string"
						},
						"certification": {
							"$ref": "#/definitions/non-empty-string",
							"enum": [
								"L0",
								"L1"
							]
						},
						"specVersion": {
							"type": "array",
							"minItems": 1,
							"items": [
								{
									"type": "string",
									"enum": [
										"0.9.5",
										"1.0.0"
									]
								}
							]
						},
						"deviceSubId": {
							"type": "array",
							"minItems": 1,
							"items": [
								{
									"type": "string",
									"enum": [
										"0",
										"1",
										"2",
										"3"
									]
								}
							]
						},
						"callbackId": {
							"$ref": "#/definitions/non-empty-string"
						},
						"deviceId": {
							"$ref": "#/definitions/non-empty-string"
						},
						"deviceCode": {
							"$ref": "#/definitions/non-empty-string"
						}
					},
					"required": [
						"env",
						"digitalId",
						"purpose",
						"serviceVersion",
						"deviceStatus",
						"firmware",
						"certification",
						"specVersion",
						"deviceSubId",
						"callbackId",
						"deviceId",
						"deviceCode"
					]
				},
				"error": {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"errorCode": {
							"$ref": "#/definitions/non-empty-string",
							"enum": [
								"0"
							]
						},
						"errorInfo": {
							"$ref": "#/definitions/non-empty-string",
							"enum": [
								"Success"
							]
						}
					},
					"required": [
						"errorCode",
						"errorInfo"
					],
					"definitions": {
						"non-empty-string": {
							"type": "string",
							"minLength": 1
						}
					}
				}
			},
			"required": [
				"deviceInfo",
				"error"
			]
		}
	],
	"definitions": {
		"non-empty-string": {
			"type": "string",
			"minLength": 1
		}
	}
}