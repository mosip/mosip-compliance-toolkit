{
	"$schema": "https://json-schema.org/draft/2019-09/schema#",
	"$id": "http://mosip-compliance-toolkit.com/schemas/DiscoverResponseSchema.json",
	"type": "array",
	"title": "Discover Response Schema",
	"description": "Discover Response Schema",
	"additionalProperties": false,
	"minItems": 1,
	"items": [
		{
			"type": "object",
			"properties": {
				"deviceId": {
					"$ref": "#/definitions/non-empty-string"
				},
				"deviceStatus": {
					"type": "string",
					"enum": [
						"Ready",
						"Busy",
						"Not Ready",
						"Not Registered"
					]
				},
				"certification": {
					"type": "string",
					"enum": [
						"L0",
						"L1"
					]
				},
				"deviceSubId": {
					"type": "array",
					"minItems": 1,
					"items": [
						{
							"type": "integer",
							"enum": [
								0,
								1,
								2,
								3
							]
						}
					]
				},
				"callbackId": {
					"$ref": "#/definitions/non-empty-string"
				},
				"digitalId": {
					"$ref": "/schemas/DigitalIdResponseSchema"
				},
				"deviceCode": {
					"$ref": "#/definitions/non-empty-string"
				},
				"specVersion": {
					"type": "array",
					"items": [
						{
							"type": "string",
							"enum": [
								"0.9.5",
								"1.0.0"
							]
						}
					]
				},
				"purpose": {
					"type": "string",
					"enum": [
						"Auth",
						"Registration"
					]
				},
				"error": {
					"$ref": "/schemas/ErrorInfoSchema"
				}
			},
			"required": [
				"deviceId",
				"deviceStatus",
				"certification",
				"callbackId",
				"digitalId",
				"deviceCode",
				"specVersion",
				"purpose",
				"error"
			]
		}
	],
	"definitions": {
		"non-empty-string": {
			"type": "string",
			"minLength": 1
		}
	}
}