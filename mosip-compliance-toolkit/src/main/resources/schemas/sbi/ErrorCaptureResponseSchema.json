							]
						},
						"data": {
							"type": [
								"string",
								"null"
							]
						},
						"hash": {
							"type": [
								"string",
								"null"
							]
						},
						"sessionKey": {
							"type": [
								"string",
								"null"
							]
						},
						"thumbprint": {
							"type": [
								"string",
								"null"
							]
						},
						"error": {
							"type": "object",
