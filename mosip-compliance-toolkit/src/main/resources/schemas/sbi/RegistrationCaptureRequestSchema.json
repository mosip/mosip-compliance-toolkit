{
	"$schema": "https://json-schema.org/draft/2019-09/schema#",
	"$id": "http://mosip-compliance-toolkit.com/schemas/RegistrationCapureRequestSchema.json",
	"title": "Registration Capture Request Schema",
	"description": "Registration Capture Request Schema",
	"type": "object",
	"properties": {
		"env": {
			"type": "string",
			"enum": [
				"Staging",
				"Developer",
				"Pre-Production",
				"Production"
			]
		},
		"purpose": {
			"type": "string",
			"enum": [
				"Registration"
			]
		},
		"specVersion": {
			"type": "string",
			"enum": [
				"0.9.5",
				"1.0.0"
			]
		},
		"timeout": {
			"$ref": "#/definitions/non-empty-string",
			"pattern": "[0-9]$"
		},
		"captureTime": {
			"type": "string",
			"format": "date-time",
			"description": "We expect YYYY-MM-ddTHH:mm:SSZ"
		},
		"transactionId": {
			"$ref": "#/definitions/non-empty-string"
		},
		"bio": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"type": {
						"type": "string",
						"enum": [
							"Finger",
							"Iris",
							"Face"
						]
					},
					"count": {
						"type": "number"
					},
					"bioSubType": {
						"type": "array",
						"uniqueItems": true
					},
					"exception": {
						"type": "array",
						"uniqueItems": true
					},
					"requestedScore": {
						"type": "string",
						"pattern": "^[0-9]+$",
						"minLength": 1,
						"maxLength": 3
					},
					"deviceId": {
						"$ref": "#/definitions/non-empty-string"
					},
					"deviceSubId": {
						"type": "string",
						"enum": [
							"0",
							"1",
							"2",
							"3"
						]
					},
					"previousHash": {
						"type": "string"
					}
				},
				"definitions": {
					"non-empty-string": {
						"type": "string",
						"minLength": 1
					}
				},
				"allOf": [
					{
						"if": {
							"properties": {
								"type": {
									"const": "Finger"
								}
							}
						},
						"then": {
							"allOf": [
								{
									"if": {
										"properties": {
											"deviceSubId": {
												"const": "1"
											}
										}
									},
									"then": {
										"properties": {
											"bioSubType": {
												"items": {
													"type": [
														"string",
														"null"
													],
													"enum": [
														"Left IndexFinger",
														"Left MiddleFinger",
														"Left RingFinger",
														"Left LittleFinger"
													]
												}
											},
											"exception": {
												"items": {
													"type": [
														"string",
														"null"
													],
													"enum": [
														"Left IndexFinger",
														"Left MiddleFinger",
														"Left RingFinger",
														"Left LittleFinger"
													]
												}
											},
											"count": {
												"minimum": 1,
												"maximum": 4
											}
										}
									}
								},
								{
									"if": {
										"properties": {
											"deviceSubId": {
												"const": "2"
											}
										}
									},
									"then": {
										"properties": {
											"bioSubType": {
												"items": {
													"type": [
														"string",
														"null"
													],
													"enum": [
														"Right IndexFinger",
														"Right MiddleFinger",
														"Right RingFinger",
														"Right LittleFinger"
													]
												}
											},
											"exception": {
												"items": {
													"type": [
														"string",
														"null"
													],
													"enum": [
														"Right IndexFinger",
														"Right MiddleFinger",
														"Right RingFinger",
														"Right LittleFinger"
													]
												}
											},
											"count": {
												"minimum": 1,
												"maximum": 4
											}
										}
									}
								},
								{
									"if": {
										"properties": {
											"deviceSubId": {
												"const": "3"
											}
										}
									},
									"then": {
										"properties": {
											"bioSubType": {
												"items": {
													"type": [
														"string",
														"null"
													],
													"enum": [
														"Right Thumb",
														"Left Thumb"
													]
												}
											},
											"exception": {
												"items": {
													"type": [
														"string",
														"null"
													],
													"enum": [
														"Right Thumb",
														"Left Thumb"
													]
												}
											},
											"count": {
												"minimum": 1,
												"maximum": 2
											}
										}
									}
								}
							]
						}
					},
					{
						"if": {
							"properties": {
								"type": {
									"const": "Iris"
								}
							}
						},
						"then": {
							"allOf": [
								{
									"if": {
										"properties": {
											"deviceSubId": {
												"const": "1"
											}
										}
									},
									"then": {
										"properties": {
											"bioSubType": {
												"items": {
													"type": [
														"string",
														"null"
													],
													"enum": [
														"Left"
													]
												}
											},
											"exception": {
												"items": {
													"type": [
														"null"
													]
												}
											},
											"count": {
												"minimum": 1,
												"maximum": 1
											}
										}
									}
								},
								{
									"if": {
										"properties": {
											"deviceSubId": {
												"const": "2"
											}
										}
									},
									"then": {
										"properties": {
											"bioSubType": {
												"items": {
													"type": [
														"string",
														"null"
													],
													"enum": [
														"Right"
													]
												}
											},
											"exception": {
												"items": {
													"type": [
														"null"
													]
												}
											},
											"count": {
												"minimum": 1,
												"maximum": 1
											}
										}
									}
								},
								{
									"if": {
										"properties": {
											"deviceSubId": {
												"const": "3"
											}
										}
									},
									"then": {
										"properties": {
											"bioSubType": {
												"items": {
													"type": [
														"string",
														"null"
													],
													"enum": [
														"Left",
														"Right"
													]
												}
											},
											"exception": {
												"items": {
													"type": [
														"string",
														"null"
													],
													"enum": [
														"Left",
														"Right"
													]
												}
											},
											"count": {
												"minimum": 1,
												"maximum": 2
											}
										}
									}
								}
							]
						}
					},
					{
						"if": {
							"properties": {
								"type": {
									"const": "Face"
								}
							}
						},
						"then": {
							"properties": {
								"bioSubType": {
									"type": "array",
									"items": {
										"type": [
											"null"
										]
									}
								},
								"exception": {
									"items": {
										"type": [
											"string",
											"null"
										],
										"enum": [
											"Left IndexFinger",
											"Left MiddleFinger",
											"Left RingFinger",
											"Left LittleFinger",
											"Left Thumb",
											"Right IndexFinger",
											"Right MiddleFinger",
											"Right RingFinger",
											"Right LittleFinger",
											"Right Thumb",
											"Left",
											"Right",
											"UNKNOWN"
										]
									}
								},
								"count": {
									"minimum": 1,
									"maximum": 1
								},
								"deviceSubId": {
									"enum": [
										"0"
									]
								}
							}
						}
					}
				]
			},
			"required": [
				"type",
				"count",
				"bioSubType",
				"requestedScore",
				"deviceId",
				"deviceSubId",
				"previousHash"
			]
		},
		"customOpts": {
			"type": [
				"null",
				"object"
			]
		}
	},
	"required": [
		"env",
		"purpose",
		"specVersion",
		"timeout",
		"captureTime",
		"transactionId",
		"bio"
	],
	"definitions": {
		"non-empty-string": {
			"type": "string",
			"minLength": 1
		}
	}
}