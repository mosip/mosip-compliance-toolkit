{
	"$schema": "https://json-schema.org/draft/2019-09/schema#",
	"$id": "http://mosip-compliance-toolkit.com/schemas/RegistrationCapureRequestSchema.json",
	"title": "Registration Capture Request Schema",
	"description": "Registration Capture Request Schema",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"env": {
			"type": "string",
			"enum": [
				"Staging",
				"Developer",
				"Pre-Production",
				"Production"
			]
		},
		"purpose": {
			"type": "string",
			"enum": [
				"Auth",
				"Registration"
			]
		},
		"specVersion": {
			"type": "string",
			"enum": [
				"0.9.5",
				"1.0.0"
			]
		},
		"timeout": {
			"$ref": "#/definitions/non-empty-string",
			"pattern": "[0-9]$"
		},
		"captureTime": {
			"type": "string",
			"format": "date-time",
			"description": "We expect YYYY-MM-ddTHH:mm:SSZ"
		},
		"transactionId": {
			"$ref": "#/definitions/non-empty-string"
		},
		"bio": {
			"type": "array",
			"items": [{
				"type": "object",
				"properties": {
					"type": {
						"type": "string",
						"enum": [
							"Finger",
							"Iris",
							"Face"
						]
					},
					"count": {
						"type": "number",
						"maximum": 4
					},
					"bioSubType": {
						"type": "array",
						"items": {
							"type": "string"
						},
						"uniqueItems": true
					},
					"exception": {
						"type": "array"
					},
					"requestedScore": {
						"type": "string",
						"pattern": "^[0-9]+$",
						"minLength": 1,
						"maxLength": 3
					},
					"deviceId": {
						"$ref": "#/definitions/non-empty-string"
					},
					"deviceSubId": {
						"type": "string",
						"enum": [
							"0",
							"1",
							"2",
							"3"
						]
					},
					"previousHash": {
						"type": "string"
					}
				},
				"allOf": [{
					"if": {
						"properties": {
							"type": {
								"const": "Finger"
							}
						}
					},
					"then": {
						"properties": {
							"bioSubType": {
								"type": "array",
								"items": {
									"enum": [
										"Left IndexFinger",
										"Left MiddleFinger",
										"Left RingFinger",
										"Left LittleFinger",
										"Left Thumb",
										"Right IndexFinger",
										"Right MiddleFinger",
										"Right RingFinger",
										"Right LittleFinger",
										"Right Thumb"
									]
								}
							},
							"exception": {
								"type": "array",
								"items": {
									"enum": [
										"Left IndexFinger",
										"Left MiddleFinger",
										"Left RingFinger",
										"Left LittleFinger",
										"Left Thumb",
										"Right IndexFinger",
										"Right MiddleFinger",
										"Right RingFinger",
										"Right LittleFinger",
										"Right Thumb"
									]
								}
							},
							"count": {
								"minimum": 1,
								"maximum": 4
							}
						}
					}
				},
					{
						"if": {
							"properties": {
								"type": {
									"const": "Iris"
								}
							}
						},
						"then": {
							"properties": {
								"bioSubType": {
									"type": "array",
									"items": {
										"enum": [
											"Left",
											"Right"
										]
									}
								},
								"exception": {
									"type": "array",
									"items": {
										"enum": [
											"Left",
											"Right"
										]
									}
								},
								"count": {
									"minimum": 1,
									"maximum": 2
								}
							}
						}
					},
					{
						"if": {
							"properties": {
								"type": {
									"const": "Face"
								}
							}
						},
						"then": {
							"properties": {
								"bioSubType": {
									"type": "array",
									"items": {
										"enum": [
											null
										]
									}
								},
								"exception": {
									"type": "array",
									"items": {
										"enum": [
											"Left IndexFinger",
											"Left MiddleFinger",
											"Left RingFinger",
											"Left LittleFinger",
											"Left Thumb",
											"Right IndexFinger",
											"Right MiddleFinger",
											"Right RingFinger",
											"Right LittleFinger",
											"Right Thumb",
											"Left",
											"Right"
										]
									}
								},
								"count": {
									"minimum": 1,
									"maximum": 1
								}
							}
						}
					}
				],
				"required": [
					"type",
					"count",
					"bioSubType",
					"exception",
					"requestedScore",
					"deviceId",
					"deviceSubId",
					"previousHash"
				]
			}]
		},
		"customOpts": {
			"type": [
				"object",
				"null",
				"string"
			]
		}
	},
	"required": [
		"env",
		"purpose",
		"specVersion",
		"timeout",
		"captureTime",
		"transactionId",
		"bio"
	],
	"definitions": {
		"non-empty-string": {
			"type": "string",
			"minLength": 1
		}
	}
}